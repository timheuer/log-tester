{
  "name": "log-tester",
  "displayName": "log-tester",
  "publisher": "timheuer",
  "description": "Demonstrates difference between OutputChannel and LogOutputChannel",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timheuer/log-tester/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timheuer/log-tester"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "log-tester.startLogging",
        "title": "Start Logging"
      },
      {
        "command": "log-tester.reportIssue",
        "title": "Report Issue",
        "icon": "$(bug)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "log-tester.reportIssue",
          "when": "true",
          "group": "2_help"
        }
      ],
      "issue/reporter": [
        {
        "command": "log-tester.reportIssue",
        "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.3",
    "@types/node": "18.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vscode/test-electron": "^2.3.6",
    "eslint": "^8.52.0",
    "glob": "^11.0.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2"
  }
}
